import java.util.*;
class NPZ1 
{
public static void main(String arg[])
{
Scanner s=new Scanner(System.in);
int a=s.nextInt();
if(a<0)
{
System.out.println("Given number is Negative");
}
else if(a>0)
{
System.out.println("Given number is Positive");
}
else
{
System.out.println("Given number is Zero");
}
}
}
